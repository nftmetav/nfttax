    "@mui/utils": "^5.2.3",
    "@vitejs/plugin-react": "^1.1.4",
    "@redux-devtools/extension": "^3.0.6",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.17.2",
    "history": "^5.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router": "^6.2.1",
    "react-router-dom": "^6.2.1",
    "react-table": "^7.7.0",
    "react-transition-group": "^4.4.2",
    "redux": "^4.1.2",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.1"
  },
  "devDependencies": {
    "@rollup/plugin-replace": "^3.0.1",
    "@types/node": "^17.0.8",
    "@types/react-dom": "^17.0.11",
    "autoprefixer": "^10.4.1",
    "postcss": "^8.4.5",
    "tailwindcss": "^3.0.8",
    "vite": "^2.7.2"
